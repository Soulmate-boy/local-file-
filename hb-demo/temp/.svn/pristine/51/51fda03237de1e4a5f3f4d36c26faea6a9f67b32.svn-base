body {
	background: #fff;
}

p {
	margin: 0;
}

.main {
	box-sizing: border-box;
	width: 100%;
	height: 4.06rem;
	background: url("../../../image/zzc.png") no-repeat;
	background-size: 100% 100%;
	color: #fff;
	box-shadow: 0 0 0.2rem rgba(0,0,0,0.3);
	position: fixed;
	top: 0;
	z-index: 20;
	padding-top:0.6rem;
}


header.mui-bar-nav {
	height: 1.94rem;
	background-size: 100% 100%;
	padding-top: 0.6rem;
	background: transparent;
	box-shadow: none;
}

header.mui-bar-nav a {
	color: white;
}

header.mui-bar-nav a>span {
	font-size: 0.6rem;
}

header.mui-bar-nav h1 {
	line-height: 1.34rem;
	font-size: 0.6rem;
	color: white;
}

.main .head {
	width: 9.62rem;
	height: 2.13rem;
	text-align: center;
	margin: 1.33rem auto 0;
}

.head p.rate {
	font-size: 0.56rem;
	color: #FFF;
	line-height: 0.5rem;
	margin-top: 0.3rem;
}

.head p.rate>span:nth-child(1) {
	margin-right: 0.32rem;
}

.head .hr {
	width: 100%;
	border-top: 0.06rem solid #fff;
	margin: 0.3rem 0;
}

.head p.date-time {
	font-size: 0.46rem;
	color: #FFF;
	line-height: 0.46rem;
}

.contain {
	margin-top: 3.46rem;
	padding-top: 1.04rem;
	width: 100%;
	box-sizing: border-box;
}

.sign {
	width: 100%;
	height: 1.44rem;
	line-height: 1.44rem;
	padding-left: 0.82rem;
	/* border: 1px solid red; */
	font-size: 0.46rem;
	color: #333333;
	box-shadow: 0 0 0.2rem rgba(0,0,0,0.3);
	position: relative;
}

.sign:before {
	display: block;
	content: "â”ƒ";
	width: 0.09rem;
	height: 0.85rem;
	color: #1f4bc5;
	position: absolute;
	left: 0.38rem;
}

.product {
	height: 1.04rem;
	line-height: 1.04rem;
	font-size: 0.46rem;
	color: #333333;
	padding: 0 0.94rem;
}

.ul-list {
	padding: 0 0.94rem;
}

.ul-item {
	height: 1.44rem;
	width: 100%;
	box-sizing: border-box;
	display: flex;
	display: -webkit-flex;
	justify-content: space-between;
	align-items: center;
	border-bottom: 0.01rem solid #CECECE;
}

.ul-item>p {
	font-size: 0.46rem;
	color: #333333;
}

.ul-item>div {
	font-size: 0.46rem;
	color: #1f4bc5;
}
