	<script>
		mui.init();

		mui('.mui-bar-tab').on('tap', 'a', function() {
			window.top.location.href = this.href;
		});

		$(function() {
			if (window.localStorage.getItem('userId') == null || window.localStorage.getItem('userId') == '') {
				mui.openWindow({
					url: './mine/login.html',
					show: {
						autoShow: true, //页面loaded事件发生后自动显示，默认为true
						aniShow: 'slide-in-right', //页面显示动画，默认为”slide-in-right“；
						duration: 200 //页面动画持续时间，Android平台默认100毫秒，iOS平台默认200毫秒；
					},
				})
			};
			$.ajax({
				type: "POST",
				url: api + '/',
				data: {
					userId: window.localStorage.getItem('userId'),
				},
				dataType: "json",
				success: function(data) {
					if (data.code == 0) {
						mui.toast('加载失败~');
						return false;
					}
					$("#total").text(data.data.total);
				},
				error: function(error) {
					console.log(error);
					window.location.href = '../404.html';
				}
			})
			$.ajax({
				type: "POST",
				url: api + '/user/wallet ',
				data: {
					userId: window.localStorage.getItem('userId'),
				},
				dataType: "json",
				success: function(data) {
					console.log(data)
					if (data.code == 0) {
						mui.toast('加载失败~');
						return false;
					}
					$("#eakeCount").text(data.data.userWallet.eakeAvailableCount);
				},
				error: function(error) {
					console.log(error);
					window.location.href = '../404.html';
				}
			});
		})

		var timer = null;

		function clearCache() {
			if (timer !== null) {
				if (new Date().getTime() - timer < 120000) {
					mui.toast("缓存已清空,请稍后重试！");
				} else if (new Date().getTime() - timer > 120000) {
					timer = null;
				}
			} else {
				$(".mui-loading").css("display", "block");
				setTimeout(function() {
					$(".mui-loading").css("display", "none");
					mui.toast('成功清除缓存!', {
						duration: 'short',
						type: 'div'
					});
				}, Math.random() * 600 + 600);
				timer = new Date().getTime();
			}

		}

		$(".more").on('click', function() {
			mui.toast('敬请期待！')
		})


