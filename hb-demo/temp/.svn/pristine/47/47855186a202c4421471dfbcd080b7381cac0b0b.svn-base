.mui-bar-nav {
	height: 1.93rem;
	background: url("../../../image/geren%20(1).png")no-repeat;
	background-size: 100% 100%;
	box-shadow: 0px 1px 15px rgba(14, 20, 84, 0.3);
	padding-top: 0.6rem;
}

.mui-bar-nav a,
.mui-bar-nav h1 {
	color: white;
}

.mui-bar-nav h1 {
	line-height: 1.34rem;
	font-size: 0.6rem;
}

.mui-input-group:after, .mui-input-group:before{
	background: transparent;
}
body {
	background: #fff;
}

p {
	margin: 0;
}


.info {
	margin-top: 1.93rem;
	/* border: 1px solid #000000; */
	position: relative;
	width: 100%;
	height: 5.3rem;
	background-size: 100% 100%;
	text-align: center;
}

.info .bg {
	position: absolute;
	top: 0;
	left: 0;
	width: 100%;
	height: 5.3rem;
	filter: drop-shadow(0 0 0.1rem rgba(0, 0, 0, 0.1)) drop-shadow(0 0 0.1rem rgba(0, 0, 0, 0.1)) drop-shadow(0 0 0.1rem rgba(0, 0, 0, 0.1));
	-webkit-filter: drop-shadow(0 0 0.1rem rgba(0, 0, 0, 0.1)) drop-shadow(0 0 0.1rem rgba(0, 0, 0, 0.1)) drop-shadow(0 0 0.1rem rgba(0, 0, 0, 0.1));
	z-index: -1;
}

.info .type-icon {
	width: 2.03rem;
	height: 2.03rem;
	margin: 0.52rem 0 0.3rem 0;
}

.info p {
	font-size: 0.44rem;
	color: #333;
}

.info-detail {
	margin-top: 0.96rem;
	width: 100%;
	/* height: 5.05rem; */
	box-shadow: 0 0 0.2rem rgba(0, 0, 0, 0.2);
	padding: 0.3rem 0.45rem;
}

.retract {
	text-indent: 2em;
	font-size: 0.46rem;
	line-height: 0.6rem;
	color: #999;
	white-space: normal;
	display: -webkit-box;
	/** 对象作为伸缩盒子模型显示 **/
	word-break: break-all;
	/* text-overflow: ellipsis; */
	/* -webkit-box-orient: vertical; */
	/** 设置或检索伸缩盒对象的子元素的排列方式 **/
	/* -webkit-line-clamp: 3; */
	/** 显示的行数 **/
	/* overflow: hidden; */
}

.retract span {
	margin-right: 0.3rem;
}

.go-back>a {
	display: block;
	font-size: 0.46rem;
	color: #fff;
	width: 9.7rem;
	height: 1.23rem;
	line-height: 1.23rem;
	background: url("../../../image/btn-long.png") no-repeat center/contain;
	color: #fff;
	text-align: center;
	margin-left: 50%;
	transform: translateX(-50%);
	margin-top: 2rem;
}
