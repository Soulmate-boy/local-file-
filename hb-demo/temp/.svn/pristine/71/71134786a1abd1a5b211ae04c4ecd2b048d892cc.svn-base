body {
	background: #fff;
}

p {
	margin: 0;
}

.main {
	box-sizing: border-box;
	width: 100%;
	height: 4.26rem;
	background: url("../../../image/zzc.png") no-repeat;
	background-size: 100% 100%;
	color: #fff;
	box-shadow: 0 0 0.2rem rgba(0, 0, 0, 0.3);
	position: fixed;
	top: 0;
	z-index: 20;
	padding-top: 0.6rem;
}

header.mui-bar-nav {
	height: 1.94rem;
	background-size: 100% 100%;
	padding-top: 0.6rem;
	background: transparent;
	box-shadow: none;
}

header.mui-bar-nav a {
	color: white;
}

header.mui-bar-nav a>span {
	font-size: 0.6rem;
}

header.mui-bar-nav h1 {
	line-height: 1.34rem;
	font-size: 0.6rem;
	color: white;
}

.main .head {
	width: 9.62rem;
	height: 2.13rem;
	text-align: center;
	margin: 1.33rem auto 0;
	display: flex;
	display: -webkit-flex;
	flex-flow: column nowrap;
	justify-content: space-around;
	text-align: left;
	/* border: 1px solid red; */
}

.head p span {
	font-size: 0.5rem;
	color: #FFF;
	line-height: 0.6rem;
}


.head .hr {
	width: 100%;
	border-top: 0.06rem solid #fff;
}


.contain {
	margin-top: 4.5rem;
	width: 100%;
	box-sizing: border-box;
	/* box-shadow: 0px -10px 15px rgba(14, 20, 84, 0.3); */
	box-shadow: 0px -5px 15px -10px rgba(0, 0, 0, 0.3);
}

.sign {
	width: 100%;
	height: 2.04rem;
	line-height: 2.04rem;
	padding-left: 0.82rem;
	/* border: 1px solid red; */
	font-size: 0.46rem;
	color: #333333;
	position: relative;
}

.sign:before {
	display: block;
	content: "â”ƒ";
	width: 0.09rem;
	height: 0.85rem;
	color: #1f4bc5;
	position: absolute;
	left: 0.38rem;
}

.product {
	height: 1.04rem;
	line-height: 1.04rem;
	font-size: 0.46rem;
	color: #333333;
	padding: 0 0.94rem;
}

.ul-list {
	padding: 0 0.94rem;
}

.ul-item {
	display: block;
	height: 1.44rem;
	width: 100%;
	box-sizing: border-box;
	display: flex;
	display: -webkit-flex;
	justify-content: space-between;
	align-items: center;
	border-bottom: 0.01rem solid #CECECE;
}

.ul-item:last-child {
	border-bottom: none;
}

.ul-item>p {
	font-size: 0.46rem;
	color: #999999;
}

.ul-item>div>span {
	font-size: 0.36rem;
	color: #999;
}

.ul-item>div>span:nth-child(1) {
	margin-right: .78rem;
}

.ul-item>div>span:nth-child(2) {
	position: relative;
	right: -0.4rem;
	font-size: 0.5rem;
	font-weight: bold;
}
